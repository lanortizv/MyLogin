<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lan.mylogin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/log"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"
            android:screenOrientation="portrait"/>
        <activity android:name=".RegistroUser"
            android:screenOrientation="portrait"
            android:label="@string/registro.user"/>
        <activity android:name=".RegistroCuenta"
            android:screenOrientation="portrait"
            android:label="@string/registro.cuenta"/>
        <activity
            android:screenOrientation="portrait"
            android:name=".Lista"
            android:label="@string/title_activity_lista"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>